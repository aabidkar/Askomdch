<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Askomdch App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">
	<listeners>
		<listener
			class-name="com.qa.askomdch.listeners.ExtentReportListeners" />
		<listener
			class-name="com.qa.askomdch.listeners.TestAllureListener" />
		<listener
			class-name="com.qa.askomdch.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Login Page Test">
		<classes>
			<class name="com.qa.askomdch.tests.LoginPageTest" />
		</classes>
	</test>
	<test name="ProductInfo Page Test">
		<classes>
			<class name="com.qa.askomdch.tests.ProductInfoPageTest" />
		</classes>
	</test>
	<test name="Register Page Test">
		<classes>
			<class name="com.qa.askomdch.tests.RegisterPageTest" />
		</classes>
	</test>
	<test name="Account Page Test">
		<classes>
			<class name="com.qa.askomdch.tests.AccountPageTest" />
		</classes>
	</test>
</suite>